@charset "UTF-8";
/* CSS Document */

/* * * * * * * GLOBAL * * * * * */	
* {
	margin: 0;
	padding: 0;
}
body {
font-family: "Lucida Grande", Arial, sans-serif;
font-size:65%;
line-height:2em;
text-align:center;
}

p {}
a {}
a:hover {text-decoration:none;}

li {list-style-type:none;}
img {border:0}

h1 span, h2 span, h3 span, h4 span {
	display: none
}

h1{}
h2{}
h3{}
h4{}

.container {
width:800px;
text-align: left;
margin: 10px auto;
padding:1em;
}

.clear {
	clear: both;
}


/* * * * * * * HEADER * * * * * */	
.header {height:14em;}
.header h1 {padding:.5em;font-size:3em;font-family:Georgia, Times, Serif;}

/* * * * * * * NAV * * * * * */	
.nav {margin:1em 0 1em 0;height:3em;}
.nav ul {}
.nav li {float:left;}
.nav li a {white-space:nowrap;display:block;float:left;padding:.3em .6em .3em .6em;text-decoration:none;}
.nav p {}
.content {}

/* * * * * * * SUBNAV * * * * * */	
.subnav {}
.subnav ul {}
.subnav li {}
.subnav a {}
.subnav a:hover {}

/* * * * * * * CONTENT * * * * * */	
.content {min-height:400px;padding:1em}
.content h2{}
.content p {}

/* * * * * * * FOOTER * * * * * */	
.footer {margin-top:3em;}

/* * * * * * * BLOGS * * * * * */	
.blog {}
.blog .index {float:left;width:30%;}
.blog .detail {float:left;width:70%;}

/* * * * * * * SLIDESHOWS * * * * * */	
.slideshow {}
.slideshow .index {float:left;width:30%;}
.slideshow .index h2 {}
.slideshow .index ul {}
.slideshow .index ul li {}
.slideshow .index ul li a {}

.slideshow .detail {float:left;width:70%}
.slideshow .detail h3 {}
.slideshow .detail .slides {}
.slideshow .detail .slides .thumbnails {}
.slideshow .detail .slides .thumbnails img {}
.slideshow .detail .slides .thumbnails a {}

/* * * * * * * COLORS * * * * * */	
body {background:#222;}
.container {background:#444;}
a {color:#ddd;}
p {color:#333}
h1{color:#fff;}
.header {background:#0099ff}
.header h1 {color:#fff;}
.nav {background:#444;}
.nav li {}
.nav li a {color:#999;}
.nav li a.On {color:#999;background:#555}
.nav li a:hover {color:#fff;background:#0099ff}
.content {background:#eee;color:#666;}
.content a {color:#333;}

